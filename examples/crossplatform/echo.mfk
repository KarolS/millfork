
#if CBM_64
    import c64_basic
#elseif CBM_264
    import c264_basic
#elseif CBM_VIC || ZX_SPECTRUM || NEC_PC_88
    // no imports needed
#else
    #error Unsupported platform
#endif

import stdio

void main() {
    pointer line
    while true {
        line = readline()
        // empty line is read as a single space on C64
        if line[0] == 32 && line[1] == 0 { return }
        if line[0] == 0 { return }
        putstrz(line)
        new_line()
    }
}
